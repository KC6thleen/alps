<aside class="aside u-spacing--double">
  <div class="u-padding--secondary u-spacing--double">
    {% include '@molecules/forms/content-filter-expand.twig' %}

    {% for content in aside_content %}
      <h3 class="u-font--secondary--m u-theme--color--darker">{{ content.aside_heading }}</h3>
      {% for item in content.aside_blocks %}
        {% include '@molecules/blocks/media-block.twig' with {'block_inner_class': 'block__row--small-to-large'} %}
      {% endfor %}
    {% endfor %}

    <div class="u-spacing">
      <h3 class="u-font--secondary--m u-theme--color--darker">More News</h3>
      {% for item in content_blocks %}
        <div class="content__block">
          <h3 class="u-theme--color--base u-font--secondary--m">{{ item.text.heading | raw }}</h3>
          <p>Proin dictum lobortis luctus. Sed sagittis massa id blandit aliquet. {% if item.text.cta %} <a href="{{ item.url }}" class="u-font--secondary--s u-text-transform--upper u-theme--color--darker"><strong>{{ item.text.cta | raw}}</strong></a> {% endif %}</p>
        </div>
        <hr>
      {% endfor %}
    </div>
  </div>
</aside>
