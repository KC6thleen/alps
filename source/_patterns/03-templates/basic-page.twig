<div class="l-wrap">
  <div class="l-wrap__content l-content" role="document">
    {% include '@organisms/global/header.twig' %}
    <main class="l-main u-spacing--double u-padding--double--bottom" role="main">
      {% include '@organisms/sections/page-header.twig' %}
      <section class="l-grid--7-col l-grid--7-col--l--offset-1 l-grid-wrap--6-of-7 u-spacing--double--until-xxlarge u-padding--zero">
        <article class="c-article l-grid-item l-grid-item--l--4-col l-grid-item--xl--3-col">
          <div class="c-article__body u-spacing text">
            {% include '@organisms/content/page-content.twig' %}
          </div>
        </article>
        <div class="l-sidebar l-grid-item l-grid-item--l--4-col l-grid-item--xl--2-col u-spacing--double u-padding--zero--sides">
          {% if breakout_block %}
            {% include '@molecules/blocks/breakout-block.twig' %}
          {% endif %}
          {% include '@organisms/asides/aside.twig' %}
        </div> <!-- /.l-sidebar -->
      </section>
    </main> <!-- /.l-main -->
    {% include '@organisms/global/footer.twig' %}
  </div> <!-- /.l-content -->
  {% include '@organisms/asides/sabbath.twig' %}
</div><!-- ./l-wrap -->
